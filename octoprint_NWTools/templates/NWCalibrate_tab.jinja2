
<div class="row-fluid">
<div class="span4">
<h1>Firmware</h1>
<button class="btn btn-primary" data-bind="enable: !$root.isPrinting(), click: updateFirmware" style="width:80%">{{ _('Update<br/>Firmware') }}</button>
&nbsp;
</div>
<div class="span7">
<h1>Sensor Setup</h1>
<button class="btn btn-primary" data-bind="enable: $root.isOperational() && !$root.isPrinting(), click: calibrateSensor" style="width:30%">{{ _('Calibrate<br/>Sensors') }}</button>
&nbsp;
<button class="btn btn-primary" data-bind="enable: $root.isOperational() && !$root.isPrinting(), click: autoCalibrate" style="width:30%">{{ _('Test<br/>Sensor 0') }}</button>
&nbsp;
<button class="btn btn-primary" data-bind="enable: $root.isOperational() && !$root.isPrinting(), click: autoCalibrate" style="width:30%">{{ _('Test<br/>Sensor 1') }}</button>
&nbsp;
</div>
</div>
<div class="row-fluid">
<div class="span4">
<h1>Bed Leveling</h1>
<button class="btn btn-primary" data-bind="enable: false, click: levelBed" style="width:40%">{{ _('Level<br/>Bed 60C') }}</button>
&nbsp;
<button class="btn btn-primary" data-bind="enable: false, click: levelBed2" style="width:40%">{{ _('Level<br/>Bed 100C') }}</button>
</div>
<div class="span7">
<h1>Bed Calibration</h1>
<button class="btn btn-primary" data-bind="enable: $root.isOperational() && !$root.isPrinting(), click: calibrateBed" style="width:30%">{{ _('Calibrate<br/>Bed 60C') }}</button>
&nbsp;
<button class="btn btn-primary" data-bind="enable: $root.isOperational() && !$root.isPrinting(), click: calibrateBed2" style="width:30%">{{ _('Calibrate<br/>Bed 100C') }}</button>
&nbsp;
<button class="btn btn-primary" data-bind="enable: $root.isOperational() && !$root.isPrinting(), click: resetCalibration" style="width:30%">{{ _('Reset<br/>Calibration') }}</button>
</div>
</div>
<br>
